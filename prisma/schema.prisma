datasource db {
  provider = "sqlite"
  url      = "file:../data/database.db"
}

generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"

  previewFeatures = ["driverAdapters", "queryCompiler"]
}

model User {
  id   Int    @id @default(autoincrement())
  name String
}
