datasource db {
  provider = "sqlite"
  url      = env("PRISMA_DATABASE_URL")
}

generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"

  previewFeatures = ["driverAdapters", "queryCompiler"]
}

model Tomato {
  id   Int    @id @default(autoincrement())
  name String
}
